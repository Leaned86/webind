             ng-model="vm.customerData.address"
             maxlength="100"
             placeholder="vm.customerData.address"
             class="form-control"  rows="4"/>

      <span style="color:#cc0000" ng-show="licence.placeOfDeliverance.$dirty && licence.placeOfDeliverance.$invalid">
